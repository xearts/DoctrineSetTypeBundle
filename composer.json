{
    "name": "raksul/doctrine-set-type-bundle",
    "type": "symfony-bundle",
    "description": "Provides support of MySQL SET type for Doctrine2 in Symfony2 applications.",
    "keywords": ["bundle", "symfony", "doctrine", "MySQL", "SET", "type", "DBAL"],
    "homepage": "https://github.com/raksul/DoctrineSetTypeBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yuichi Okada",
            "email": "y.okada@raksul.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/symfony": "~2.8|~3.0",
        "doctrine/orm": "~2.3"
    },
    "require-dev": {
        "phake/phake": "^3.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Raksul\\DoctrineSetTypeBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    }
}
