{
    "name": "raksul/doctrine-set-type-bundle",
    "type": "symfony-bundle",
    "description": "Provides support of MySQL SET type for Doctrine2 in Symfony2 applications.",
    "keywords": ["bundle", "symfony", "doctrine", "MySQL", "SET", "type", "DBAL"],
    "homepage": "https://github.com/raksul/DoctrineSetTypeBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yuichi Okada",
            "email": "y.okada@raksul.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "doctrine/doctrine-bundle": "~1.11 || ^2.0",
        "doctrine/common": "~2.8",
        "doctrine/orm": "~2.6",
        "symfony/config": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/doctrine-bridge": "^4.3",
        "symfony/framework-bundle": "^4.3",
        "symfony/http-kernel": "^4.3",
        "symfony/validator": "^4.3"
    },
    "require-dev": {
        "phake/phake": "^3.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Raksul\\DoctrineSetTypeBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    }
}
